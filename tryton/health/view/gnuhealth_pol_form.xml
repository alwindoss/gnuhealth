<?xml version="1.0"?>
<form>
    <group id="pol_header" colspan="4" col="8">
        <label name="person"/>
        <field name="person"/>
        <label name="page_type"/>
        <field name="page_type"/>
        <label name="page_date"/>
        <field name="page_date"/>
        <label name="relevance"/>
        <field name="relevance"/>
        <newline/>
        <label name="age"/>
        <field name="age"/>
        <label name="institution"/>
        <field name="institution"/>
        <newline/>
        <label name="summary"/>
        <field name="summary" colspan="4"/>
    </group>
    <newline/>
    <separator string="Context" id="separator_context" colspan="4"/>
    <newline/>
    <group id="pol_body" colspan="4" col="6">
        <label name="medical_context"/>
        <field name="medical_context"/>
        <label name="social_context"/>
        <field name="social_context"/>
    </group>

    <newline/>

    <group id="pol_medical" colspan="4" col="6">
        <label name="health_condition"/>
        <field name="health_condition"/>
        <field name="health_condition_text"/>
        <label name="health_condition_code"/>
        <field name="health_condition_code"/>
        <newline/>
        <label name="procedure"/>
        <field name="procedure"/>
        <field name="procedure_text"/>
        <label name="procedure_code"/>
        <field name="procedure_code"/>
        <newline/>
        <label name="gene"/>
        <field name="gene"/>
        <label name="natural_variant"/>
        <field name="natural_variant"/>
        <label name="phenotype"/>
        <field name="phenotype"/>
    </group>

    <newline/>

    <separator string="Extended Information" id="separator_extra_info" colspan="4"/>
    <newline/>
    <field name="info" colspan="4"/>
    <newline/>
    <separator string="Measurements" id="separator_measurements" colspan="4"/>
    <field name="measurements" colspan="4"/>
    <group id="pol_footer" colspan="4" col="10">
        <label name="page"/>
        <field name="page"/>
        <label name="federation_account"/>
        <field name="federation_account"/>
        <label name="author"/>
        <field name="author"/>
        <label name="author_acct"/>
        <field name="author_acct"/>
        <label name="node"/>
        <field name="node"/>
    </group>

</form>
